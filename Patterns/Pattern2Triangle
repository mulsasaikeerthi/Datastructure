class Solution {

    void printTriangle(int n) {
        for(int i=0;i<2*n-1;i++){
            for(int j=0; j<=i && i<n;j++){
                System.out.print("* ");
            }
            for(int k=n-1;k>i-n && i>=n;k--){
                System.out.print("* ");
            }
                        System.out.println();
        }
    }
}
or Patteren10
class Solution {

    void printTriangle(int n) {
        for(int i=0;i<2*n-1;i++){
            int stars=i;
            if(i>=n){
                stars=2*(n-1)-i;
            }
            for(int j=0;j<=stars;j++){
                System.out.print("* ");
            }
            System.out.println();
        }
    }
}
class Solution {

    void printTriangle(int n) {
        for(int i=0;i<n;i++){
            for(int j=0;j<=i;j++){
                if(i%2==0){
                    if(j%2==0){
                    System.out.print("1 ");
                }
                else{
                    System.out.print("0 ");
                }
                } 
                
                 if(i%2!=0){
                    if(j%2==0){
                 System.out.print("0 ");
                }
                else{
                   System.out.print("1 ");
                }
                 } 
            }
            System.out.println();
        }
    }
}
